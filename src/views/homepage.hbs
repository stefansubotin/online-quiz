<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>App</title>
    <script src="https://js.pusher.com/8.2.0/pusher.min.js"></script>
    <script>
      // Enable pusher logging - don't include this in production
      Pusher.logToConsole = true;

      var pusher = new Pusher('cefecd31795a4e419288', {
        cluster: 'eu'
      });

      var channel = pusher.subscribe('my-channel');
      channel.bind('my-event', function(data) {
        document.getElementById("divTest").innerHTML = JSON.stringify(data);
      });
    </script>
  </head>
  <body>
    <div id="divTest">Hello</div><br><br>
    <input type="text" id="testInput" name="testInput"><br><br>
    <button onclick="onButtonClick()">Test</button>
    <script>
      function onButtonClick(){
  	    document.getElementById("divTest").innerHTML = document.getElementById("testInput").value;
      };
    </script>
  </body>
</html>